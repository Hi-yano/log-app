<h1>Area#new</h1>
<p>Find me in app/views/area/new.html.erb</p>

<%= link_to 'トップページに戻る', root_path %>
<%= form_with(model: @log, local: true) do |f| %>
  <div>
    <p>date</P>
    <%= f.text_field :date, placeholder:"例）2020-08-01" %>
  </div>
  <div>
    <p>area</P>
    <%= f.text_field :area, placeholder:"例）石垣島" %>
  </div>
  <div>
    <p>entry</P>
    <%= f.collection_select(:entry, Entry.all, :id, :name, {}) %>
  </div>
  <%= f.submit "記録する", class:"sell-btn" %>
<% end %>